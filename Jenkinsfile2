#!/usr/bin/env groovy

@Library('pipeline-library-demo')_  //master or whatever branch

pipeline{

      agent any
      tools {
           maven 'maven'
           jdk 'java-11'
       }
        
        stages{

              stage('maven build'){
                  steps{
                      script{
		    	                sh "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 50)
                    }
                }
		
           }	       	     	         
}
